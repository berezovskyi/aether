  h1: a#top.displaced-anchor Aether VoID Statistics Viewer
  .col-sm-6
    h3 View statistics for:
    .form-group(ng-class='{ "has-error": !sparqlEndpointInputValid && sparqlEndpointInputHasFocus, "has-success": sparqlEndpointInputValid && sparqlEndpointInputHasFocus }')
      label.control-label SPARQL Endpoint:
      input.form-control(type='text', ng-model='sparqlEndpointInput', ng-blur='sparqlEndpoint = sparqlEndpointInputValid ? sparqlEndpointInput : sparqlEndpoint ; sparqlEndpointInputHasFocus = !sparqlEndpointInputValid', ng-focus='sparqlEndpointInputHasFocus = true')
    .form-group
      label.control-label Graph:
      div(ng-class='graphIRIFetching && "input-group"')
        select.form-control(ng-model='graphIRI', ng-readonly='graphIRIFetching', ng-options='graph.graphIRI.value as (graph.graphIRI.value || "default") + " (" + ((graph.datasets.value>0 && (graph.datasets.value | number)+" VoID descriptions, ") || "") + (graph.triples.value | number) + " triples)" for graph in graphs')
        span.input-group-addon(ng-show='graphIRIFetching')
          i.glyphicon.glyphicon-refresh.fa-spin
    .form-group
      label.control-label Dataset:
      div(ng-class='datasetIRIFetching && "input-group"')
        select.form-control(ng-model='datasetIRI', ng-readonly='datasetIRIFetching', ng-options='dataset.datasetIRI.value as dataset.datasetIRI.value for dataset in datasets')
          option(value='') endpoint itself
        span.input-group-addon(ng-show='datasetIRIFetching')
          i.glyphicon.glyphicon-refresh.fa-spin
  .col-sm-6
    h3 Compare against:
    .form-group(ng-class='{ "has-error": !compar_sparqlEndpointInputValid && compar_sparqlEndpointInputHasFocus, "has-success": compare_sparqlEndpointInputValid && compare_sparqlEndpointInputHasFocus }')
      label.control-label SPARQL Endpoint:
      input.form-control(type='text', ng-model='compare_sparqlEndpointInput', ng-blur='compare_sparqlEndpoint = compare_sparqlEndpointInputValid ? compare_sparqlEndpointInput : compare_sparqlEndpoint ; compare_sparqlEndpointInputHasFocus = !compare_sparqlEndpointInputValid', ng-focus='compare_sparqlEndpointInputHasFocus = true')
    .form-group
      label.control-label Graph:
      div(ng-class='compare_graphIRIFetching && "input-group"')
        select.form-control(ng-model='compare_graphIRI', ng-readonly='compare_graphIRIFetching', ng-options='compare_graph.graphIRI.value as (compare_graph.graphIRI.value || "default") + " (" + ((compare_graph.datasets.value>0 && (compare_graph.datasets.value | number)+" VoID descriptions, ") || "") + (compare_graph.triples.value | number) + " triples)" for compare_graph in compare_graphs')
        span.input-group-addon(ng-show='compare_graphIRIFetching')
          i.glyphicon.glyphicon-refresh.fa-spin
    .form-group
      label.control-label Dataset:
      div(ng-class='compare_datasetIRIFetching && "input-group"')
